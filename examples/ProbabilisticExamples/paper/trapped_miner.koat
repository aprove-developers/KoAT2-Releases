(GOAL EXPECTEDCOMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS f))
(VAR I FLAG N)
(RULES
  f(I,FLAG,N) -{0;0}> g(0,FLAG,N)
  g(I,FLAG,N) -{0;0}> 0.5:h(I,1,N) :+: 0.5:g(I+1,FLAG,N) :|: I<N
  # Trapped position
  h(I,FLAG,N) -{0;0}> [1/3]:i(I,FLAG,N) :+: [1/3]:j(I,FLAG,N) :+: [1/3]:k(I,FLAG,N) :|: FLAG>0
  h(I,FLAG,N) -{0;0}> g(I+1,FLAG,N)
  # Generate Costs
  i(I,FLAG,N) -{3;3}> h(I,0,N)
  j(I,FLAG,N) -{5;5}> h(I,1,N)
  k(I,FLAG,N) -{7;7}> h(I,1,N)
)
