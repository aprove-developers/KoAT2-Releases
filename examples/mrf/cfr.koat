(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS l0))
(VAR A B C D)
(RULES
  l0(A,B,C,D) -> Com_1(l0a(A,B,C,D))
  l0a(A,B,C,D) -> Com_1(l0b(A,B,C,D))
  l0b(A,B,C,D) -> Com_1(l1(A,B,C,D)) 
  l1(A,B,C,D) -> Com_1(l3(A,B,C,D)) :|: A < 0
  l1(A,B,C,D) -> Com_1(l2(A,B,C,D)) :|: A >= 0
  l2(A,B,C,D) -> Com_1(l1(A,B + 1,C,D)) :|: B <= C
  l2(A,B,C,D) -> Com_1(l1(A - 1,B,C,D)) :|: B > C
  l3(A,B,C,D) -> Com_1(l4(A,B,C,D)) :|: C > 0
  l4(A,B,C,D) -> Com_1(l4(A,B,C - 1,D)) :|: C > 0
)
