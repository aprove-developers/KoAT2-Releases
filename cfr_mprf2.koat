(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS l0))
(VAR X Y A)
(RULES
  l0(X,Y) -> Com_1(l1(A,Y)) :|: A >= 1
  l1(X,Y) -> Com_1(l2(X + 1,Y)) :|: Y > 0

  l1(X,Y) -> Com_1(l1a(X + 1,Y)) :|: 19 >= X && 1 <= X
  l1a(X,Y) -> Com_1(l1a(X + 1,Y)) :|: 19 >= X && 1 <= X
  
  l1a(X,Y) -> Com_1(l2(X + 1,Y)) :|: Y > 0
  l2(X,Y) -> Com_1(l1(X,Y - 1)) 
)
